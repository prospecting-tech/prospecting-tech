{
  "name": "Crawls Through LinkedIn To Find Our Target Audience :)",
  "short_name": "Crawls Through LinkedIn To Find Our Target Audience :)",
  "version": "1.01",
  "manifest_version": 2,
  "content_scripts": [
    {
      "js": ["jquery.min.js", "content.js"],
      "matches": ["https://www.linkedin.com/*"],
      "run_at": "document_end"
    }
  ],
  "browser_action": {
    "default_title": "Leadspod Crawler",
    "default_popup": "popup.html"
  },
  "permissions": ["tabs","storage"]
}
